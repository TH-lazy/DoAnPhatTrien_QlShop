<body>
    <div class="container-fluid">
        <div class="header">
            <div class="nav">
                <div class="logo">
                    <img src="~/Assets/image/Capture.PNG" alt="logo">
                </div>
                <div class="navright">
                    <div class="simple-search">
                        <input type="text" placeholder="Nhập thông tin." />
                        <button type="submit">Search</button>
                    </div>
                    <div class="btncart">
                        <button type="submit" class="cart-form-submit"><i class="ti-shopping-cart"></i> Giỏ Hàng(<span class-cart-noti>0</span>)</button>
                    </div>
                    <div class="account">
                        <button type="submit" class="account-form-submit"><i class="ti-user"></i> Tài khoản</button>

                        <ul class="account-menu">
                            <li class="account-item"><a href="#">Tài khoản của tôi</a></li>
                            <li class="account-item"><a href="#">Thoát</a></li>
                        </ul>

                    </div>
                </div>
            </div>

            <div class="nav2">
                <ul id="navcontent">
                    <li><a href="#">Trang chủ</a></li>
                    <li>
                        <a href="#">
                            <i class="ti-menu"></i> Dành cho nam <i class="ti-angle-double-down"></i>
                        </a>
                        <ul class="subnav">
                            <li><a href="#">Áo khoác</a></li>
                            <li><a href="#">Áo hoodie</a></li>
                            <li><a href="#">Áo sơ mi</a></li>
                            <li><a href="#">Áo thun</a></li>
                            <li><a href="#">Quần jean</a></li>
                            <li><a href="#">Quần dài</a></li>
                            <li><a href="#">Quần short</a></li>
                        </ul>
                    </li>
                    <li>
                        <a href="#">
                            <i class="ti-menu"></i> Dành cho nữ <i class="ti-angle-double-down"></i>
                        </a>
                        <ul class="subnav">
                            <li><a href="#">Áo khoác</a></li>
                            <li><a href="#">Áo hoodie</a></li>
                            <li><a href="#">Áo sơ mi</a></li>
                            <li><a href="#">Áo thun</a></li>
                            <li><a href="#">Quần jean</a></li>
                            <li><a href="#">Quần dài</a></li>
                            <li><a href="#">Váy dài</a></li>
                            <li><a href="#">Chân Váy</a></li>
                        </ul>
                    </li>
                    <li>
                        <a href="#">
                            <i class="ti-menu"></i> Khác <i class="ti-angle-double-down"></i>
                        </a>
                        <ul class="subnav">
                            <li><a href="#">Balo - Túi</a></li>
                            <li><a href="#">Phụ kiện</a></li>
                            <li><a href="#">Giày - dép</a></li>
                        </ul>
                    </li>
                    <li><a href="#">Giới thiệu</a></li>
                    <li><a href="#">Khuyến mãi</a></li>
                </ul>
            </div>
        </div>
    </div>

    <div class="model">

        <div class="model-overplay"></div>
        <div class="model-body">
            <form class="login-form" autocomplete="off" action="#">
                <p class="title-frm">Đăng nhập</p>
                <input type="email" name="email" placeholder="Email" />
                <div class="input-icon">
                    <input type="password" name="email" placeholder="Password" />
                    <i class="fa fa-eye show-password"></i>
                </div>
                <a href="#" class="forgot">Forgot password?</a>
                <button>Sign in</button>
            </form>
        </div>
    </div>

    <div class="container">
        <div class="row slider-cr">
            <div class="col-md-12">
                <div id="carouselExampleIndicators" class="carousel slide" data-bs-ride="carousel">
                    <div class="carousel-indicators">
                        <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="0" class="active" aria-current="true" aria-label="Slide 1"></button>
                        <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="1" aria-label="Slide 2"></button>
                        <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="2" aria-label="Slide 3"></button>
                        <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="3" aria-label="Slide 4"></button>
                        <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="4" aria-label="Slide 5"></button>
                    </div>
                    <div class="carousel-inner">
                        <div class="carousel-item active">
                            <img src="~/Assets/image/Data_Images/Data_Images/9f44f8b1-7919-0500-e2b6-0017deb04863.jpg" class="d-block w-100" alt="...">
                        </div>
                        <div class="carousel-item">
                            <img src="~/Assets/image/Data_Images/Data_Images/33181116-e676-0100-78ff-0017d2c5ac31.jpg" class="d-block w-100" alt="...">
                        </div>
                        <div class="carousel-item">
                            <img src="~/Assets/image/Data_Images/Data_Images/306b57ac-4527-0100-3bbe-0017ba573ac1.jpg" class="d-block w-100" alt="...">
                        </div>
                        <div class="carousel-item">
                            <img src="~/Assets/image/Data_Images/Data_Images/a6075851-6ca9-0a00-6ab8-00152feb683c.jpg" class="d-block w-100" alt="...">
                        </div>
                        <div class="carousel-item">
                            <img src="~/Assets/image/Data_Images/Data_Images/4437a856-b31a-1300-72ff-0016033a7cbc.jpg" class="d-block w-100" alt="...">
                        </div>
                    </div>
                    <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide="prev">
                        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                        <span class="visually-hidden">Previous</span>
                    </button>
                    <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide="next">
                        <span class="carousel-control-next-icon" aria-hidden="true"></span>
                        <span class="visually-hidden">Next</span>
                    </button>
                </div>

            </div>
        </div>

        <h1 class="title-sp">Danh Mục Sản Phẩm</h1>

        <div class="content">        
            <div class="row">
                @RenderBody()
            </div>
        </div>
    </div>

    <div class="container-fluid">
        <div class="footer">
            <div class="row">
                <div class="col-md-4">
                    <div class="f-info">
                        <h2><i class="ti-crown"></i> Thông tin chung </h2>
                        <p>Đồ án môn phát triển phần mềm</p>
                       
                            <p>Thành viên nhóm: </p>
                            <p>Tên: Lưu Minh Hiếu 2001181109</p>
